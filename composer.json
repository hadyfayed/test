{
  "name": "medisoft/technical-task",
  "description": "this is my technical task for medisoft which is a poker odds calculator and sting analizer in mvc design pattern that follows S.O.L.I.D principle",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Hady Fayed",
      "email": "hadyfayed@gmail.com"
    }
  ],
  "require": {
    "vlucas/phpdotenv": "3.3.2",
    "league/route": "4.2.0",
    "zendframework/zend-diactoros": "2.1.1",
    "zendframework/zend-httphandlerrunner": "1.1.0",
    "league/plates": "3.3.0",
    "league/container": "3.2.2",
    "league/booboo": "2.0",
    "mzdr/oh-snap": "1.0.0",
    "ext-json": "*",
    "puli/repository": "^1.0@dev",
    "symfony/filesystem": "^4.3@dev",
    "ext-mbstring": "*",
    "erusev/parsedown": "dev-master",
    "tightenco/collect": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "App/",
      "MediSoft\\": "App/Core/"
    },
    "files": [
      "App/Core/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.x-dev",
    "fig-r/psr2r-sniffer": "dev-master"
  }
}
